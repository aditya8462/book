<html>
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Cnompiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js">
       
    </script>
   
          
     
    <style>
        .maincontainer{
            display: flex;
            justify-content: center;
            align-items: center;
            background-color: #fff;
            
            width:100vw
        }
        .subcontainer{
            background-color:#03228f;
            border-radius: 10px;
            width:95%;
        }
        .heading{
            font-weight:bold;
            color: '#2f3640';
            font-size: 30px;
            text-align: center;
            color: #fff;

            
        }
        .msgStyle{
          display: flex;
          justify-content: center;
          align-items: center;
          color: #fff;
        }
    </style>
    <div class="maincontainer">
        <div class="subcontainer">
        <div class="heading">List Of Categorywriter <a  href="/categorywriter/" target="mw" class="waves-effect waves-teal btn-flat" style="color: #fff;"><i class="material-icons right">add</i>Add Categorywriter</a></div>
        <div class="row">
          <form action="/categorywriter/editcategorywriter" method="post" enctype="multipart/form-data">
            <table class="highlight">
                <thead>
                  <tr>
                      <th style="color: #fff;">Categorywriter Id</th>
                      <th style="color: #fff;">Publication Id</th>
                      <th style="color: #fff;">BookCategory Id</th>
                      <th style="color: #fff;">CategoryWriter Name</th>
                      <th style="color: #fff;">Description</th>
                      <th style="color: #fff;">Picture</th>    
                      <th style="color: #fff;">Action</th>
                  </tr>
                </thead>
                <tbody>
               <% data.forEach((item,index)=>{%>
              <tr>
                <td style="color: #fff;"><%=item.categorywriterid%></td>
                <td style="color: #fff;"><%=item.publicationid %></td>
                <td style="color: #fff;"><%=item.bookcategoryid%></td>
                <td style="color: #fff;"><%=item.categorywritername%></td>
                <td style="color: #fff;"><%=item.description.slice(0,50)%></td>
                <td><a title="Edit"  style="color:#fff"  href="/categorywriter/displaypicture?wid=<%=item.categorywriterid%>"><img src='/images/<%=item.picture%>' width='150px' height='20%'/></a></td>
                <td><a title="Edit"  style="color:#fff"  href="/categorywriter/displaybyid?wid=<%=item.categorywriterid%>"><span class="material-icons">
                    edit
                    </span></a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    <a title="Edit"  style="color:#fff"  href="/categorywriter/delete?wid=<%=item.categorywriterid%>"><span class="material-icons">
                    delete
                        </span></a>
                    </td>
              </tr>
              <% })%>
                </tbody>
                </table>
 
                                  </form> 
                             </div>    
               
                </div>
                </div>
                
           
    
  
     <!-- Compiled and minified CSS -->
     <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">

     <!-- Compiled and minified JavaScript -->
     
     <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
     
     </html>